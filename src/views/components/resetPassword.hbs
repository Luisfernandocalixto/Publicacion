<div class="contentSigUp">
  <h3 class="text-white mt-60 text-center">Ingrese una nueva contraseña</h3>
 <div class="contentForms">

  <form 
    action="/users/resetPassword/{{token}}"
    method="POST"
    class="formUser mt-60"

  >
    <div class="groupInput">
      <label for="email" class="textLabel">Contraseña</label>
        <div
        class="contentShowPassword"
      >
        <span id="visible"><img
            src="/img/visible.svg"
            alt="Ver"
            title="Ver"
          /></span>
        <input
          type="password"
          name="password"
          class="formInput"
          placeholder="*****"
          id="input"
          autocomplete=""
        />

      </div>
    </div>

    <div class="groupInput">
      <button
        type="submit"
        name="button"
        class="buttonPrimary w-100"
      >Enviar</button>
    </div>
  </form>

</div>
 </div>




<script type="" src="/js/sweetalert.js"></script>
<script type="module" src="/js/resetPassword.js"></script>
