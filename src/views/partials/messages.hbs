{{#if success_msg}}
  <script type="module">
    import { renameMessage} from '/js/app.js';

    Swal.fire({ title: "Bien hecho!", text: renameMessage(`{{success_msg}}`), icon: "success" ,confirmButtonColor: "#024751"
    });
  </script>
{{/if}}
