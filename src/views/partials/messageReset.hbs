


{{#if error_msg}}
<script>
  Swal.fire({ icon: "error", title: "Oops...", text: `{{error_msg}}` });
</script>
{{/if}}
{{#if errors}}
<script>
  Swal.fire({
    icon: "error", title: "Oops...", text: `
    {{#each errors}}
      {{text}}
    {{/each}}

    `});
</script>
{{/if}}
{{#if success_msg}}
<script>
  Swal.fire({
    title: "Bien hecho!", text: `{{success_msg}}`, icon: "success"
  });
</script>
{{/if}}
{{#if message}}
  <script>
    Swal.fire({ icon: "error", title: "Oops...", text: `{{message}}`});
  </script>
{{/if}}